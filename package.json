{
  "name": "paperhive-launch",
  "version": "0.0.0",
  "description": "PaperHive launch",
  "scripts": {
    "build": "gulp",
    "ci": "npm run install-deps && npm run build",
    "install-deps": "npm install && bower install"
  },
  "author": "PaperHive team <info@paperhive.org>",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "bower": "1.6.3",
    "browserify": "11.2.0",
    "browserify-shim": "3.8.10",
    "connect-history-api-fallback": "1.1.0",
    "del": "2.0.2",
    "gulp": "3.9.0",
    "gulp-angular-templatecache": "1.8.0",
    "gulp-batch": "1.0.5",
    "gulp-connect": "2.2.0",
    "gulp-htmlhint": "0.3.0",
    "gulp-htmlmin": "1.2.0",
    "gulp-if": "2.0.0",
    "gulp-imagemin": "2.3.0",
    "gulp-jscs": "3.0.1",
    "gulp-jscs-stylish": "1.2.1",
    "gulp-jshint": "1.11.2",
    "gulp-less": "3.0.3",
    "gulp-minify-css": "1.2.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-streamify": "1.0.2",
    "gulp-template": "3.0.0",
    "gulp-uglify": "1.4.2",
    "gulp-util": "3.0.6",
    "lodash": "3.10.1",
    "merge-stream": "1.0.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.4.0"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "angular-animate": "./bower_components/angular-animate/angular-animate.js",
    "angular-bootstrap-tpls": "./bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
    "angular-leaflet-directive": "./bower_components/angular-leaflet-directive/dist/angular-leaflet-directive.js",
    "angular-route": "./bower_components/angular-route/angular-route.js",
    "angular-route-segment": "./bower_components/angular-route-segment/build/angular-route-segment.js",
    "angular-sanitize": "./bower_components/angular-sanitize/angular-sanitize.js",
    "angular-simple-logger": "./bower_components/angular-simple-logger/dist/index.js",
    "detect-element-resize": "./bower_components/javascript-detect-element-resize/jquery.resize.js",
    "leaflet": "./bower_components/leaflet/dist/leaflet.js",
    "jquery": "./bower_components/jquery/dist/jquery.js",
    "ngSmoothScroll": "./bower_components/ngSmoothScroll/lib/angular-smooth-scroll.js"
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery"
      ]
    },
    "angular-animate": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-bootstrap-tpls": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-leaflet-directive": {
      "exports": null,
      "depends": [
        "angular",
        "angular-simple-logger",
        "leaflet:L"
      ]
    },
    "angular-route": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-route-segment": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-sanitize": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "detect-element-resize": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "jquery": {
      "exports": "$"
    },
    "leaflet": {
      "exports": "L"
    },
    "ngSmoothScroll": {
      "exports": null,
      "depends": [
        "angular"
      ]
    }
  }
}
